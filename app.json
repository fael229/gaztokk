    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoiZmFmYTMzIiwiYSI6ImNtMGt6Y2cxNDAwNnMya3M4YjY0M3o5bDQifQ._cOmI2HaFmzd98de2vQZug",
          "RNMapboxMapsVersion": "11.0.0"
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Show current location on map."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a2735ae1-9669-48f4-bcba-0a79ef678152"
